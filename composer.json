{
  "name": "touchsms/touchsms",
  "description": "touchSMS Api Client",
  "keywords": ["touchsms", "sms", "sms gateway", "mobile", "cellphone"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "touchSMS",
      "email": "dev@touchsms.com.au"
    }
  ],
  "autoload": {
    "psr-4": {
      "TouchSms\\ApiClient\\Api\\": "generated/",
      "TouchSms\\ApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TouchSms\\ApiClient\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "jane-php/open-api-runtime": "~7.6.0",
    "psr/http-client-implementation": "*",
    "php-http/client-common": "^1.9 || ^2.0",
    "php-http/discovery": "^1.11",
    "php-http/multipart-stream-builder": "^1.1"
  },
  "require-dev": {
    "jane-php/open-api-3": "~v7.6.0",
    "symfony/http-client": "^6.0 || ^7.0",
    "nyholm/psr7": "^1.2",
    "friendsofphp/php-cs-fixer": "^3.2.2",
    "symfony/phpunit-bridge": "^6.0 || ^7.0",
    "opis/json-schema": "^1.0",
    "symfony/console": "^6.0 || ^7.0",
    "symfony/process": "^6.0 || ^7.0"
  },
  "conflict": {
    "php-http/httplug": "< 2.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
